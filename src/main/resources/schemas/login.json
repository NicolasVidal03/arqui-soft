{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Login",
    "description": "A user loggin",
    "type": "object",
    "properties": {
        "email": {
            "description": "The unique email identifier for a user",
            "type": "string"
        },
        "password": {
            "type": "string",
            "minimum": 0,
            "exclusiveMaximum": 15
        }
    },
    "required": ["email", "password"]
}